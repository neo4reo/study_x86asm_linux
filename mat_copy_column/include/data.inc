section .bss


    ; - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
    ; B = destination matrix with size 3x3
    ; - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

    align 16, resb 1
    B:    resd (3*3)


section .data


    ; - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
    ; A = source matrix with size 3x3
    ; - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

    align 16, db 0
    A:    ;row 0
          dd __float32__(1.0) ;A[0,0]
          dd __float32__(2.0) ;A[0,1]
          dd __float32__(3.0) ;A[0,2]

          ;row 1
          dd __float32__(4.0) ;A[1,0]
          dd __float32__(5.0) ;A[1,1]
          dd __float32__(6.0) ;A[1,2]
 
          ;row 2
          dd __float32__(7.0) ;A[2,0]
          dd __float32__(8.0) ;A[2,1]
          dd __float32__(9.0) ;A[2,2]
